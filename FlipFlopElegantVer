package classExercises;

public class FlipFlopElegantVer {

	public static void main(String[] args) {
		for (int i = 1; i <= 100; i++) {
			String s = "";
			if (i % 3 == 0) {
				s = "Flip";
			}
			if (i % 5 == 0) {
				s += "Flop";
			}
			if (s.length() == 0) {
				s = String.valueOf(i);
			}
			System.out.println(s);
		}
	}

}
